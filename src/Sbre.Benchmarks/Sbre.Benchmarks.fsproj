    <ItemGroup>
      <ProjectReference Include="..\Sbre\Sbre.fsproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="BenchmarkDotNet" Version="0.13.2" />
    </ItemGroup>

</Project>
