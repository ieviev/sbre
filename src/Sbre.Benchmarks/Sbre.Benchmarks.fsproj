        <TargetFramework>net7.0</TargetFramework>
        <OtherFlags>$(OtherFlags) --optimize+ --extraoptimizationloops:1</OtherFlags>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="Twain.fs" />
        <Compile Include="LongParagraph.fs" />
        <Compile Include="FullMtwain.fs" />
